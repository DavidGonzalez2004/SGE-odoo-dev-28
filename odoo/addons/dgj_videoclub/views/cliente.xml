<odoo>
  <data>

    <record id="dgj_videoclub.cliente_form" model="ir.ui.view">
      <field name="name">dgj_videoclub.cliente.form</field>
      <field name="model">dgj_videoclub.cliente</field>
      <field name="arch" type="xml">
        <form string="Cliente">
          <sheet>
            <group>
              <field name="name"></field>
              <field name="telefono"></field>
              <field name="correo_electronico"></field>
              <field name="direccion"></field>
              <field name="ejemplar_ids">
                <tree string="Ejemplar">
                  <field name="numero_ejemplar"></field>
                  <field name="formato"></field>
                  <field name="pelicula_id"></field>
                </tree>
              </field>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="dgj_videoclub.cliente_tree" model="ir.ui.view">
      <field name="name">dgj_videoclub.cliente.tree</field>
      <field name="model">dgj_videoclub.cliente</field>
      <field name="arch" type="xml">
        <tree string="Cliente">
          <field name="name"></field>
          <field name="telefono"></field>
          <field name="correo_electronico"></field>
          <field name="direccion"></field>
          <field name="ejemplar_ids"></field>
        </tree>
      </field>
    </record>

  </data>
</odoo>